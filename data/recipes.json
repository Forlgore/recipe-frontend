    {
      "id": 2,
      "name": "Creamy Lemon Dill Chicken & Rice Casserole",
      "servings": 4,
      "tags": ["chicken", "casserole", "lemon", "dill", "oven", "rice"],
      "components": [
        {
          "component_name": "Main",
          "ingredients": [
            { "amount": "500 g", "item": "chicken breast", "key": "chicken-breast", "notes": "cubed" },
            { "amount": "1 cup", "item": "uncooked brown rice", "key": "brown-rice" },
            { "amount": "2 cups", "item": "chicken broth (unsalted)", "key": "chicken-broth" },
            { "amount": "1 cup", "item": "plain Greek yogurt", "key": "plain-greek-yogurt" },
            { "amount": "1 tbsp", "item": "fresh dill", "key": "dill", "notes": "chopped" },
            { "amount": "1 tbsp", "item": "lemon zest", "key": "lemon-zest" },
            { "amount": "2 tbsp", "item": "lemon juice", "key": "lemon-juice" },
            { "amount": "1 cup", "item": "spinach", "key": "spinach", "optional": true },
            { "amount": "1 tsp", "item": "garlic powder", "key": "garlic-powder" },
            { "amount": "1 tsp", "item": "onion powder", "key": "onion-powder" },
            { "amount": null, "item": "salt", "key": "salt" },
            { "amount": null, "item": "pepper", "key": "pepper" }
          ],
          "instructions": [
            "Preheat oven to 190°C / 375°F.",
            "In a baking dish, combine rice and chicken broth.",
            "In a bowl, whisk yogurt, dill, lemon zest, lemon juice, garlic powder, and onion powder.",
            "Add cubed chicken to the mixture and fold to coat.",
            "Pour chicken mixture over rice. Cover tightly with foil.",
            "Bake 45–50 minutes, until rice is cooked.",
            "Stir in spinach (optional), season with salt and pepper, and serve warm."
          ]
        }
      ],
